<!DOCTYPE html>
<html>
<head>
  <title>Journal : Home</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_pack_tag 'application' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
  <%= render partial: "application/nav" %>
  <div class="container-fluid">
    <%= yield %>
  </div>

  <% flash.each do |type, msg| %>
  <div>
    <%= msg %>
  </div>
  <% end %>

</body>
</html>
